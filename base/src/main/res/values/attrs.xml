    <declare-styleable name="BottomTab">
        <attr name="menu" format="reference" />
        <attr name="topMargin" format="dimension" />
        <attr name="marginBetweenImg2Text" format="dimension" />
        <attr name="defaultCheckPos" format="integer" />
        <attr name="imgFactor" format="float" />
        <attr name="textSize" format="dimension" />
        <attr name="scaleFactor" format="float" />
        <!--        是否开始时立即进行一次pos回调-->
        <attr name="callbackImmediately" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ArcImageView">
        <attr name="arcHeight" format="dimension" />
    </declare-styleable>


    <declare-styleable name="UBaseView">
        <attr name="cx" format="dimension" /> <!-- 圆心x坐标 -->
        <attr name="cy" format="dimension" /> <!-- 圆心y坐标 -->
        <attr name="mass" format="float" /> <!-- 质量 -->
        <attr name="volume" format="float" /> <!-- 体积 -->
        <attr name="density" format="float" /> <!-- 密度 -->
        <attr name="vx" format="float" /> <!-- 速度x分量 -->
        <attr name="vy" format="float" /> <!-- 速度y分量 -->
        <attr name="ax" format="float" /> <!-- 加速度x分量 -->
        <attr name="ay" format="float" /> <!-- 加速度y分量 -->
        <attr name="e" format="float" /> <!-- 弹性系数 -->
        <attr name="color" format="color" /> <!-- 颜色 -->

    </declare-styleable>


</resources>